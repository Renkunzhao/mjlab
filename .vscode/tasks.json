{
    "version": "2.0.0",
    "inputs": [
        {
        "id": "task",
        "type": "pickString",
        "description": "Select task",
        "options": [
            "Mjlab-Hopping-Flat-Unitree-Go2", 
            "Mjlab-Tracking-Flat-Unitree-G1",
            "Mjlab-Tracking-Flat-Unitree-G1-No-State-Estimation",
            "Mjlab-Velocity-Rough-Unitree-G1",
            "Mjlab-Velocity-Flat-Unitree-G1",
            "Mjlab-Velocity-Rough-Unitree-Go1",
            "Mjlab-Velocity-Flat-Unitree-Go1",
            "Mjlab-Velocity-Rough-Unitree-Go2",
            "Mjlab-Velocity-Flat-Unitree-Go2"
        ],
        "default": "Mjlab-Hopping-Flat-Unitree-Go2"
        },
        {
        "id": "motion",
        "type": "pickString",
        "description": "Select task",
        "options": [
            "LAFAN1-g1-jumps1_subject1", 
        ],
        "default": "LAFAN1-g1-jumps1_subject1"
        },
    ],  
    "tasks": [
        {
            "label": "List env",
            "type": "shell",
            "command": "uv run list_envs",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Train",
            "type": "shell",
            "command": "MUJOCO_GL=egl uv run train ${input:task} --registry-name kzren-uw-madison/wandb-registry-Motions/${input:motion} --env.scene.num-envs 4096",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Play G1",
            "type": "shell",
            "command": "uv",
            "args": [
                "run",
                "play",
                "Mjlab-Velocity-Flat-Unitree-G1",
                "--num_envs", "32",
                // "--viewer","viser",
                "--wandb-run-path",
                "kzren-university-of-wisconsin-madison/mjlab/gwb1irkk",
                "--checkpoint_file",
                "logs/rsl_rl/g1_velocity/2025-11-07_18-52-00/model_2000.pt"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Play Go1",
            "type": "shell",
            "command": "uv",
            "args": [
                "run",
                "play",
                "Mjlab-Velocity-Rough-Unitree-Go1",
                "--num_envs", "32",
                "--wandb-run-path",
                "kzren-university-of-wisconsin-madison/mjlab/8b5lgt1u",
                // "--checkpoint_file",
                // "logs/rsl_rl/go1_velocity/2025-11-08_04-49-24/model_9999.pt"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Play Go2",
            "type": "shell",
            "command": "uv",
            "args": [
                "run",
                "play",
                "Mjlab-Velocity-Rough-Unitree-Go2",
                // "--agent", "random",
                "--num_envs", "32",
                // "--viewer","viser",
                "--wandb-run-path",
                "kzren-university-of-wisconsin-madison/mjlab/v4j4layr",
                // "--checkpoint_file",
                // "logs/rsl_rl/go2_velocity/2025-11-08_04-49-24/model_9999.pt"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Hopping Go2 Train",
            "type": "shell",
            "command": "MUJOCO_GL=egl uv run train Mjlab-Hopping-Flat-Unitree-Go2 --env.scene.num-envs 4096",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
        {
            "label": "Hopping Go2 play",
            "type": "shell",
            "command": "uv",
            "args": [
                "run",
                "play",
                "Mjlab-Hopping-Flat-Unitree-Go2",
                // "--agent", "random",
                "--num_envs", "32",
                // "--viewer","viser",
                "--wandb-run-path",
                // "kzren-university-of-wisconsin-madison/mjlab/z033t6n1",  // Hopping Init, flat walking
                // "kzren-university-of-wisconsin-madison/mjlab/rl50w0vp",     // Height
                "kzren-university-of-wisconsin-madison/mjlab/wlvdfr3x"        // Height
                // "--checkpoint_file",
                // "logs/rsl_rl/go2_hopping/2025-11-12_23-48-32/model_0.pt"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "scripts",
            "problemMatcher": []
        },
    ]
}
